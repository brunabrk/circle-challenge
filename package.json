{
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "prepare": "husky install",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "postinstall": "patch-package",
    "eas-build-pre-install": "./eas-hooks/eas-build-pre-install.sh",
    "eas-build-on-success": "./eas-hooks/eas-build-on-success.sh",
    "test:e2e:ios": "detox test --configuration ios.sim.debug --take-screenshots failing --record-videos failing",
    "test:e2e:android": "detox test --configuration android.emu.debug --take-screenshots failing --record-videos failing"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-navigation/bottom-tabs": "6.2.0",
    "@react-navigation/native": "6.0.8",
    "@react-navigation/stack": "6.1.1",
    "@reduxjs/toolkit": "^1.6.2",
    "dayjs": "^1.10.6",
    "expo": "~48.0.15",
    "expo-asset": "~8.9.1",
    "expo-av": "~13.2.1",
    "expo-blur": "~12.2.2",
    "expo-constants": "~14.2.1",
    "expo-file-system": "~15.2.2",
    "expo-font": "~11.1.1",
    "expo-linking": "~4.0.1",
    "expo-navigation-bar": "~2.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-tracking-transparency": "~3.0.3",
    "expo-web-browser": "~12.1.1",
    "jest": "^29.2.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.7",
    "react-native-calendars": "^1.1267.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-web": "~0.18.10",
    "react-redux": "^7.2.5",
    "redux-persist": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^5.0.1",
    "@react-native-community/eslint-config": "^3.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.24",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.64.12",
    "@types/react-native-calendars": "^1.1264.2",
    "@types/uuid": "^8.3.1",
    "cypress": "^8.7.0",
    "cz-conventional-changelog": "3.3.0",
    "detox": "^20.7.0",
    "detox-expo-helpers": "^0.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-native": "^3.11.0",
    "expo-detox-hook": "^1.0.10",
    "husky": "^7.0.2",
    "jest-expo": "^48.0.2",
    "lint-staged": "^11.2.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "**/*.ts?(x)": [
      "bash -c tsc -p tsconfig.json --noEmit"
    ]
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "name": "heylinda-app",
  "version": "1.0.0"
}
